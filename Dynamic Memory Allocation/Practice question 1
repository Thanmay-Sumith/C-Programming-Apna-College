//Write a program to allocate memory to store 5 prices.

#include <stdio.h>
#include<stdlib.h>

int main()
{
    float *ptr;
    ptr = (float *) malloc(5 * sizeof(float));
    
    ptr[0] = 100.0;
    ptr[1] = 200.0;
    ptr[2] = 300.0;
    ptr[3] = 400.0;
    ptr[4] = 500.0;
    
    for(int i =0; i<5;i++){
        printf("%f\n",ptr[i]);
    }

    return 0;
}
