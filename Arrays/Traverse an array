// Traverse an array.
#include <stdio.h>

int main()
{
    int aadhar[5];
    
    //INPUT
    int *ptr = &aadhar[0];
    for(int i=0;i < 5;i++){
        printf("%d number: ",i);
        scanf("%d\n",(ptr+i));
    }
    
    //OUTPUT
    for(int i=0;i < 5;i++){
        printf("%d number: %d\n",i,*(ptr+i));
    }
    return 0;
}
