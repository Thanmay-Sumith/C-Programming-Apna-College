// Write a function to reverse an array.
#include <stdio.h>
void reverse(int arr[],int n);
void printnum(int arr[],int n);
int main()
{
    int arr[] = {1,2,3,4,5};
    reverse(arr,5);
    printnum(arr,5);
    return 0;
}

void printnum(int arr[],int n){
    for(int i=0;i<n;i++){
        printf("%d\t",arr[i]);
    }
}
void reverse(int arr[],int n){
    for(int i=0;i<(n/2);i++){
        int FN = arr[i]; // FN is first number
        int SN = arr[n-i-1]; // SN is second number
        arr[i] = SN;
        arr[n-i-1] = FN;
    }
}
