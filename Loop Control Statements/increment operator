#include <stdio.h>

int main()
{
    //increment operator.
    // i++ (post increment)
    // ++i (pre increment)
    
    int i = 1;
    printf("%d\n",i++); // post increment
    printf("%d\n",i);
    
    int j = 1;
    printf("%d\n",++j); // pre increment
    printf("%d",j);
    
    return 0;
}
