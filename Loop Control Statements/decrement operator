#include <stdio.h>

int main()
{
    //decrement operator.
    // i-- (post decrement)
    // --i (pre decrement)
    
    int i = 1;
    printf("%d\n",i--); // post decrement
    printf("%d\n",i);
    
    int j = 1;
    printf("%d\n",--j); // pre decrement
    printf("%d",j);
    
    return 0;
}
