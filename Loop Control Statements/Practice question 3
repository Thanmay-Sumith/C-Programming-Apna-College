METHOD-1(for loop):-
//Print the table of a number, input given by the user.
#include <stdio.h>

int main()
{
    int n;
    printf("enter n: ");
    scanf("%d",&n);
    for(int i = 1; i <= 10; i++){
        printf("%d\n", n*i);
    }
    
    
    return 0;
}

METHOD-2(while loop):-
//Print the table of a number, input given by the user.
#include <stdio.h>

int main()
{
    int n;
    printf("enter n: ");
    scanf("%d",&n);
    int i = 1;
    while(i <= 10){
        printf("%d\n", n*i);
        i++;
    }
    return 0;
}

METHOD-3(do while loop):-
//Print the table of a number, input given by the user.
#include <stdio.h>

int main()
{
    int n;
    printf("enter n: ");
    scanf("%d",&n);
    int i = 1;
    do{
        printf("%d\n", n*i);
        i++;
    } while(i <= 10);
    return 0;
}
