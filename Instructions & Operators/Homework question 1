// Write a program to print average of 3 numbers.
#include <stdio.h>
int main()
{
    int x,y,z;
    printf("enter x: ");
    scanf("%d",&x);
    printf("enter y: ");
    scanf("%d",&y);
    printf("enter z: ");
    scanf("%d",&z);
    
    printf("%d is the average of three numbers", (x+y+z) / 3);
    return 0;
}
