// Write a function to convert celsius to fahrenheit.
#include <stdio.h>
float convertTemp(float celsius);

int main()
{
    float celsius;
    printf("enter temp in celsius: ");
    scanf("%f",&celsius);
    printf("The temp in fahrenheit is %f",convertTemp(celsius));
    
    return 0;
}

float convertTemp(float celsius){
    float far = celsius * (9.0/5.0) + 32;
    return far;
}
