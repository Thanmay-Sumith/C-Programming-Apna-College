//Check whether the character is present or not in the string.

#include <stdio.h>
#include<string.h>
void checkChar(char name[],char ch);

int main()
{
    char name[] = "SUMITH";
    char ch = 'u';
    checkChar(name,ch);
    return 0;
}

void checkChar(char name[],char ch){
    for(int i = 0;name[i] != '\0';i++){
        if(ch == name[i]){
            printf("Character is present");
            return;
        }
    }
    printf("Character is not present");
}
