//Find the salted form of a password entered by user if the salt is "123" & added at the end.

#include <stdio.h>
#include<string.h>
void changepass(char password[]);

int main()
{
    char password[100];
    scanf("%s",password);
    changepass(password);
    return 0;
}

void changepass(char password[]){
    char salt[] = "123";
    char newpass[200];
    
    strcpy(newpass,password);
    strcat(newpass,salt);
    puts(newpass);
    
    
}
