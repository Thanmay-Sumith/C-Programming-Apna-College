//Passing Structure to Function.

#include <stdio.h>
#include<string.h>

struct student{
    int roll;
    float cgpa;
    char name[100];
};

void printInfo(struct student s1);

int main()
{
    struct student s1 = {17,10.0,"SUMITH"};
    printInfo(s1);

    return 0;
}

void printInfo(struct student s1){
    printf("name is %s\n",s1.name);
    printf("roll number is %d\n",s1.roll);
    printf("cgpa is %f\n",s1.cgpa);
}
