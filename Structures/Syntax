#include <stdio.h>
#include<string.h>

struct student{
    int roll;
    float cgpa;
    char name[100];
};

int main()
{
  struct student s1;
  s1.roll = 17;
  s1.cgpa = 10.0;
  strcpy(s1.name,"SUMITH");
  
  printf("Name is %s\n",s1.name);
  printf("roll number is %d\n",s1.roll);
  printf("cgpa is %f",s1.cgpa);

    return 0;
}
