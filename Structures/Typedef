//Typedef.

#include <stdio.h>
#include<string.h>

typedef struct student{
    int roll;
    float cgpa;
    char name[100];
} st;

int main()
{
    st s1 = {17,10.0,"Sumith"};
    
    printf("name is %s\n", s1.name);
    printf("roll number is %d\n",s1.roll);
    printf("cgpa is %f\n",s1.cgpa);

    return 0;
}
