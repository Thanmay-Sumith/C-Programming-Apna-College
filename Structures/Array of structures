//Array of structures.

#include <stdio.h>
#include<string.h>

struct student{
    int roll;
    float cgpa;
    char name[100];
};

int main()
{
  struct student CSE[100];
  CSE[0].roll = 18;
  CSE[0].cgpa = 10.0;
  strcpy(CSE[0].name,"THANMAY");
  
  printf("name is %s\n",CSE[0].name);
  printf("roll number is %d\n",CSE[0].roll);
  printf("cgpa is %f\n",CSE[0].cgpa);

    return 0;
}
