//Create a structure to store complex numbers. (use arrow operator)

#include <stdio.h>
#include<string.h>

struct complex{
    int real;
    int img;
};

int main()
{
    struct complex c1 = {3,4};
    struct complex *ptr = &c1;
    
    printf("real part is %d\n",ptr->real);
    printf("_Imaginary part is %d\n",ptr->img);
    return 0;
}
