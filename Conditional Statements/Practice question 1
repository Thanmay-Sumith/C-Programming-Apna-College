//Write a program to check if a student passes or failed.
#include <stdio.h>

int main()
{
    int marks;
    printf("enter marks: ");
    scanf("%d",&marks);
    
    if(marks > 30 && marks <= 100){
        printf("PASS\n");
    } else if(marks <= 30 && marks >= 0){
        printf("FAIL\n");
    } else{
        printf("Not correct marks\n");
    }
    return 0;
}
