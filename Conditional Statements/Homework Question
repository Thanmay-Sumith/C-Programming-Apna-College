// Write a program to check the given number is a natural number or not.
#include <stdio.h>

int main()
{
    int n;
    printf("enter n: ");
    scanf("%d",&n);
    
    if(n >= 1){
        printf("%d is a natural number",n);
    } else {
        printf("%d is not a natural number",n);
    }

    return 0;
}
