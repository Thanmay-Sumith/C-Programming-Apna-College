//Write a program to find if a character entered by user is upper case or lower case.
#include <stdio.h>

int main()
{
    char ch;
    printf("enter the character: ");
    scanf("%c",&ch);
    
    if(ch >= 'A' && ch <= 'Z'){
        printf("UPPER CASE \n");
    } else if(ch >= 'a' && ch <= 'z'){
        printf("LOWER CASE \n");
    } else {
        printf("Case is not defined\n");
    }
    return 0;
}
