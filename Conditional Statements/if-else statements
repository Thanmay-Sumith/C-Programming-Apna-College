#include <stdio.h>

int main()
{
    int age;
    printf("enter age: ");
    scanf("%d",&age);
    
    if(age >= 18){
        printf("adult \n");
        printf("you can have license");
    } else if(age > 13 && age < 18){
        printf("Teenager");
    } else{
        printf("Child \n");
    }

    return 0;
}
